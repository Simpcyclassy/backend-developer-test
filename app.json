{
    "name": "backend-developer-test",
    "description": " API that serves the latest scores of fixtures of matches in a Mock Premier League",
    "env": {
        "MONGODB_URI": {
          "required": true
        },
        "PORT": {
          "required": true
        },
        "REDISCLOUD_URL": {
          "required": true
        },
        "SECRET": {
          "required": true
        }
    },
    "formation": {
      "web": {
        "quantity": 1
      }
    },
    "addons": [
      "mongolab",
      "rediscloud"
    ],
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      }
    ],
    "stack": "heroku-18"
}